{
    "name": "inspiredminds/contao-dynamic-ptable-backport",
    "description": "Contao extension to backport the automatic dynamic ptable support from Contao 4.10 and up to Contao 4.9.",
    "type": "contao-bundle",
    "homepage": "https://github.com/inspiredminds/contao-dynamic-ptable-backport",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "homepage": "http://www.inspiredminds.at",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/inspiredminds/contao-dynamic-ptable-backport/issues",
        "source": "https://github.com/inspiredminds/contao-dynamic-ptable-backport"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "composer/semver": "^1.0 || ^2.0 || ^3.0",
        "contao/core-bundle": "^4.9 || ^5.0",
        "jean85/pretty-package-versions": "^1.5 || ^2.0",
        "symfony/config": "^4.4 || ^5.2 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.2 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.2 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoDynamicPtableBackport\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoDynamicPtableBackport\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0"
    }
}
